function openCity(e,s){for(var t,l=document.getElementsByClassName("tabcontent"),a=0;a<l.length;a++)l[a].style.display="none";for(t=document.getElementsByClassName("tablinks"),a=0;a<t.length;a++)t[a].className=t[a].className.replace(" active","");document.getElementById(s).style.display="block",e.currentTarget.className+=" active"}$(".js-open-menu").click(function(){$(".header-menu").toggleClass("active"),$(this).toggleClass("active"),$("body, html").toggleClass("scroll-disabled"),$(window).width()<992&&$("body, html").toggleClass("scroll-disabled")}),$(".js-open-sub-menu").click(function(){$(this).hasClass("active")?$(this).removeClass("active"):($(".js-open-sub-menu").removeClass("active"),$(this).addClass("active"))}),$(".dropdown").click(function(){$(this).toggleClass("active")});