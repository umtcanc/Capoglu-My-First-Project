$(".js-open-menu").click(function(){$(".header-menu").toggleClass("active"),$("body, html").toggleClass("scroll-disabled"),$(window).width()<992&&$("body, html").toggleClass("scroll-disabled")}),$(".js-open-sub-menu").click(function(){$(this).hasClass("active")?$(this).removeClass("active"):($(".js-open-sub-menu").removeClass("active"),$(this).addClass("active"))}),$(".dropdown").click(function(){$(this).toggleClass("active"),$("html,body").toggleClass("scroll-disabled")});